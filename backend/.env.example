# Copy this file to .env and fill in the values before running the backend locally

# Backend server
PORT=8000

# MongoDB connection string (MongoDB Atlas or local)
# Example (Atlas): mongodb+srv://<user>:<password>@cluster0.abcde.mongodb.net/myrequest?retryWrites=true&w=majority
MONGODB_CONNECTION_STRING=

# JWT secret for signing tokens
JWT_SECRET=your_jwt_secret
